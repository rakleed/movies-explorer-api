{
    "env": {
        "es2023": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": [
        "error",
        { "allow": ["_id"] }
      ],
      "import/prefer-default-export": "off",
      "import/no-mutable-exports": "off",
      "import/extensions": [
        0,
        { "js": "always" }
      ]
    }
}
